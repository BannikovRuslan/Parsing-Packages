{
  "name": "ember-flexberry",
  "version": "3.8.0-beta.17",
  "description": "Ember Flexberry Addon",
  "keywords": [
    "ember-addon",
    "flexberry"
  ],
  "license": "MIT",
  "author": {
    "name": "Flexberry Team",
    "email": "mail@flexberry.net",
    "url": "http://flexberry.net/"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Flexberry/ember-flexberry.git"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:ts": "eslint --ext .ts --env node .",
    "start": "ember serve",
    "test": "ember try:each",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --no-ignore",
    "*.ts": "eslint --no-ignore --ext .ts --env node",
    "*.hbs": "ember-template-lint --config '{ \"extends\": [\"recommended\", \"stylistic\"], \"rules\": { \"no-bare-strings\": true } }'"
  },
  "dependencies": {
    "strip-bom": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1"
  },
  "resolutions": {
    "ws": "6.2.1",
    "ember-get-config": "0.3.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-flexberry-data"
    ]
  }
}
